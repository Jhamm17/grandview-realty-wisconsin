# Wisconsin MLS API Configuration (MLS Aligned API - Metro MLS)
# Get access token from: clambrou@metromls.com
NEXT_PUBLIC_WISCONSIN_MLS_API_URL=http://aligned.metromls.com/RESO/OData
WISCONSIN_MLS_ACCESS_TOKEN=your_wisconsin_mls_access_token
# Metro MLS RESO OUID (required for MLS Aligned API)
WISCONSIN_MLS_OUID=M00000662
# Application name for MLS-Aligned-User-Agent header (MLS assigned)
# You can use either of these variable names:
MLS-Aligned-User-Agent=your_application_name
# OR
WISCONSIN_MLS_APP_NAME=your_application_name

# Wisconsin Supabase Configuration
NEXT_PUBLIC_WISCONSIN_SUPABASE_URL=your_wisconsin_supabase_url
NEXT_PUBLIC_WISCONSIN_SUPABASE_ANON_KEY=your_wisconsin_supabase_anon_key
WISCONSIN_SUPABASE_SERVICE_ROLE_KEY=your_wisconsin_supabase_service_role_key

# Wisconsin Domain Configuration
NEXT_PUBLIC_WISCONSIN_BASE_URL=https://grandviewwisconsin.com

# Vercel Configuration (for redeployment)
VERCEL_TOKEN=your_vercel_token
VERCEL_PROJECT_ID=your_wisconsin_vercel_project_id
VERCEL_TEAM_ID=your_vercel_team_id

# Instagram API (if using same account)
INSTAGRAM_ACCESS_TOKEN=your_instagram_access_token

# Email Configuration (Resend)
RESEND_API_KEY=your_resend_api_key

# Build Configuration
BUILD_TIME=true
